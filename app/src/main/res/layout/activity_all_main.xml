<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.kyuubi.gr.AllMainActivity">

    <ImageButton
        android:id="@+id/ibGroup"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="46dp"
        app:layout_constraintLeft_toRightOf="@+id/ibViewPoint"
        android:layout_marginLeft="20dp" />

    <ImageButton
        android:id="@+id/ibUser"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintLeft_toRightOf="@+id/ibGroup"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="46dp" />

    <ImageButton
        android:id="@+id/ibViewPoint"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:srcCompat="@mipmap/ic_launcher"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="46dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/ibHomework"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginTop="29dp"
        app:layout_constraintTop_toBottomOf="@+id/textView27"
        app:layout_constraintLeft_toRightOf="@+id/ibCalenda"
        android:layout_marginLeft="19dp" />

    <ImageButton
        android:id="@+id/ibMessenger"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginTop="29dp"
        app:layout_constraintTop_toBottomOf="@+id/textView23"
        app:layout_constraintLeft_toRightOf="@+id/ibHomework"
        android:layout_marginLeft="21dp" />

    <ImageButton
        android:id="@+id/ibCalenda"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginTop="29dp"
        app:layout_constraintTop_toBottomOf="@+id/textView22"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginTop="0dp"
        android:text="Point View"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/ibViewPoint"
        tools:layout_editor_absoluteX="8dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="0dp"
        android:text="Calenda"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ibCalenda" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="0dp"
        android:text="Homework"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@+id/textView26"
        app:layout_constraintTop_toBottomOf="@+id/ibHomework" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="0dp"
        android:text="Messenger"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@+id/textView25"
        app:layout_constraintTop_toBottomOf="@+id/ibMessenger" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="0dp"
        android:text="User Profile"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@+id/textView27"
        app:layout_constraintTop_toBottomOf="@+id/ibUser" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="0dp"
        android:text="Group"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toRightOf="@+id/textView22"
        app:layout_constraintTop_toBottomOf="@+id/ibGroup" />
</android.support.constraint.ConstraintLayout>
